{
	"name": "nativescript-yandex-metrica",
	"version": "1.0.0",
	"description": "Nativescript Library for AppMetrica SDK",
	"main": "AppMetricaSDK",
	"typings": "index.d.ts",
	"nativescript": {
		"platforms": {
			"android": "6.0.0",
			"ios": "6.0.1"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dmitribd/nativescript-yandex-metrica.git"
	},
	"scripts": {
		"tsc": "npm i && tsc",
		"build": "npm run tsc && npm run build.native",
		"build.native": "node scripts/build-native.js",
		"tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
		"ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/platforms/**'",
		"prepack": "npm run build.native",
		"demo.ios": "npm run tsc && cd ../AppMetricaDemo && tns run ios",
		"demo.android": "npm run tsc && cd ../AppMetricaDemo && tns run android",
		"demo.reset": "cd ../AppMetricaDemo && npx rimraf -- hooks node_modules platforms package-lock.json",
		"plugin.prepare": "npm run build && cd ../AppMetricaDemo && tns plugin remove nativescript-yandex-metrica && tns plugin add ../src",
		"clean": "npm run demo.reset && npx rimraf -- node_modules package-lock.json && npm i"
	},
	"keywords": [
		"NativeScript",
		"TypeScript",
		"iOS",
		"YandexMobileMetrica"
	],
	"author": "Dmitri Bedunkevits",
	"bugs": {
		"url": "https://github.com/dmitribd/nativescript-yandex-metrica/issues"
	},
	"license": {
		"type": "MIT",
		"url": "https://github.com/dmitribd/nativescript-yandex-metrica/blob/master/LICENSE"
	},
	"homepage": "https://github.com/dmitribd/nativescript-yandex-metrica/blob/master/README.md",
	"devDependencies": {
		"tns-core-modules": "^6.0.0",
		"tns-platform-declarations": "^6.0.0",
		"typescript": "~3.6.4",
		"prompt": "^1.0.0",
		"rimraf": "^2.6.3",
		"tslint": "^5.12.1",
		"semver": "^5.6.0"
	},
	"dependencies": {},
	"bootstrapper": "nativescript-plugin-seed"
}
